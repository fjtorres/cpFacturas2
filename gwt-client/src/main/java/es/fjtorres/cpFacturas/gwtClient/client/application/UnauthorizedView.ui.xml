<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:app.html="urn:import:es.fjtorres.cpFacturas.gwtClient.client.ui.html">

	<ui:with type="es.fjtorres.cpFacturas.gwtClient.client.i18n.Messages"
		field="messages" />
		
	<g:ScrollPanel>
		<b:Container fluid="true">
			<b:Row>
				<b:Column size="SM_12">
					<b:Alert type="DANGER">
						<g:Label text="{messages.error_unauthorized}" />
					</b:Alert>
				</b:Column>
			</b:Row>
		</b:Container>
	</g:ScrollPanel>
</ui:UiBinder> 